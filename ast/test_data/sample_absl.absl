import ABSL;

var today = Context.GetCurrentUserDate();
foreach (var item in this.Employee) {
    if (item.ProcessingStatus == CLDT_PStatusCode.CO_PROCESSED) {
        raise MESSAGE.Create("W", "Item Already processed");
        return;
    }
    if (item.HireDate.GreaterThan(today)) {
        raise MESSAGE.Create("I", "Defer to Buffer");
    } else {
        item.ProcessingStatus = CLDT_PStatusCode.CO_PROCESSED;
        item.ProcessingTime = Context.GetCurrentGlobalDateTime();
        raise MESSAGE.Create("I", "Process Item");
    }
}